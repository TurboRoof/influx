    "body-parser": "^1.13.3",
    "eslint": "^1.2.1",
    "eslint-plugin-react": "^3.2.3",
    "express": "^4.13.3",
    "faucet": "0.0.1",
    "keymirror": "^0.1.1",
    "node-sass": "^3.2.0",
